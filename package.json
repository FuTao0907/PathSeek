  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,scss,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json,css,scss,md}",
    "test:coverage": "craco test --coverage --watchAll=false",
    "test:ci": "craco test --coverage --watchAll=false --passWithNoTests",
    "analyze": "npm run build && npx bundle-analyzer build/static/js/*.js",
    "prepare": "husky install",
    "quality:check": "npm run lint:check && npm run type-check && npm run format:check",
    "quality:fix": "npm run lint && npm run format",
    "quality:report": "node scripts/quality-check.js",
    "pre-commit": "npm run quality:check && npm run test:ci"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "prettier": "^3.5.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "enzyme": "^3.11.0",
    "@types/enzyme": "^3.10.18",
    "webpack-bundle-analyzer": "^4.10.1",
    "chalk": "^4.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
